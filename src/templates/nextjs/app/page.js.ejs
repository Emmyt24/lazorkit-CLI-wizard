<% if (features.includes('passkey') || features.includes('auth')) { %>import LoginForm from './LoginForm';
<% } %>
<% if (features.includes('gasless')) { %>import TransferForm from './TransferForm';
<% } %>
<% if (features.includes('pay-widget')) { %>import PayWidget from './PayWidget';
<% } %>
<% if (features.includes('token-swap')) { %>import TokenSwap from './TokenSwap';
<% } %>
<% if (features.includes('subscription')) { %>import SubscriptionService from './SubscriptionService';
<% } %>

export default function Home() {
  return (
    <main>
      <h1>Welcome to <%= appName %></h1>
      <p>Integrated with Lazorkit SDK</p>
<% if (features.includes('passkey')) { %>
      <LoginForm />
<% } %>
<% if (features.includes('gasless')) { %>
      <TransferForm />
<% } %>
<% if (features.includes('pay-widget')) { %>
      <PayWidget amount="0.1" merchant="example-merchant" />
<% } %>
<% if (features.includes('token-swap')) { %>
      <TokenSwap />
<% } %>
<% if (features.includes('subscription')) { %>
      <SubscriptionService />
<% } %>
    </main>
  );
}

